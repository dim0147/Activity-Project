@{
    ViewBag.Title = "Home Page";
    ViewBag.cssHome = "active";
}



@section introdution{
    <!-- Hero Section Begin -->
    <section class="hero">
            <div class="hero__sliders owl-carousel">
                <div class="hero__items set-bg" data-setbg="/Content/Media/Images/templates/hero/hero-1.jpg">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-8">
                                <div class="hero__text">
                                    <span>Welcome to the world of largest clubs</span>
                                    <h2>What hurts today makes you stronger tomorrow</h2>
                                    <a href="#" class="primary-btn">DISCOVER MORE</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="hero__items set-bg" data-setbg="/Content/Media/Images/templates/hero/hero-1.jpg">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-8">
                                <div class="hero__text">
                                    <span>Club Discover</span>
                                    <h2>Pick and choose your club</h2>
                                    <a href="#" class="primary-btn">DISCOVER MORE</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    <!-- Hero Section End -->
}


<!-- About Section Begin -->
<section class="home-about spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-7">
                <div class="home__about__pic">
                    <div class="home__about__pic__item large-item set-bg" data-setbg="/Content/Media/Images/templates/about/about-1.jpg"></div>
                    <div class="home__about__pic__item">
                        <div class="home__about__pic__item__inner set-bg" data-setbg="/Content/Media/Images/templates/about/about-2.jpg"></div>
                        <div class="home__about__pic__item__inner set-bg" data-setbg="/Content/Media/Images/templates/about/about-3.jpg"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-5">
                <div class="home__about__text">
                    <div class="section-title">
                        <img src="/Content/Media/Images/templates/icon.png" alt="">
                        <h2>Welcome to Club</h2>
                    </div>
                    <span>“What hurts today makes you stronger tomorrow.”</span>
                    <p>
                        We are offering internationally certified Design Club in Rishikesh and 300
                        good training in Rishikesh, India - Join us to have such life-transforming
                        experience in The world capital of Clubs.
                    </p>
                    <p class="para-2">
                        Here is a great place for those who are in search of the ancient practice and
                        true essence of activiews, meditation, health, peace and learning the art of creating a class to
                        teach safely. We are passionate about adjustments and mainly focus on the alignment of the
                        asanas.
                    </p>
                    <a href="#" class="primary-btn">MORE ABOUT US</a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- About Section End -->


<div id="root">

</div>



